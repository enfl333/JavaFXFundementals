<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="flores.javafx.recap.sessions.Screen4Controller">
   <children>
      <VBox>
         <children>
            <Label text="Simple Treeview">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </Label>
            <TreeView fx:id="treeOfTest">
               <VBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </VBox.margin>
            </TreeView>
            <HBox>
               <children>
                  <Button mnemonicParsing="false" onAction="#runSelected" text="Run" />
                  <Button mnemonicParsing="false" onAction="#stopTest" text="Stop" />
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
